{
    "doc": {
        "prefix": ["doc"],
        "body": [
            "\"\"\"$1",
            "\t$2",
            "\"\"\"",
            "$3"
        ],
        "description": "long form docs for functions and classes"
    },
    "ifmain": {
        "prefix": ["ifmain"],
        "body": [
            "if __name__ == \"__main__\":",
            "\tmain()"
        ]
    },
    "pandas": {
        "prefix": ["pd"],
        "body": [
            "import numpy as np",
            "import pandas as pd"
        ]
    },
    "numpy": {
        "prefix": ["np"],
        "body": [
            "import numpy as np"
        ]
    },
    "parquet": {
        "prefix": ["pq"],
        "body": [
            "import pyarrow as pa",
            "from pyarrow import parquet as pq"
        ]
    },
    "timing": {
        "prefix": ["timing"],
        "body": [
            "from time import perf_counter",
            "old = perf_counter()",
            "$1",
            "print(f'time spent: {perf_counter() - old}')"
        ]
    }
}
