{
    "doc": {
        "prefix": ["doc"],
        "body": [
            "\"\"\"$1",
            "\t$2",
            "\"\"\"",
            "$3"
        ],
        "description": "long form docs for functions and classes"
    },
    "ifmain": {
        "prefix": ["ifmain"],
        "body": [
            "if __name__ == \"__main__\":",
            "\tmain()"
        ]
    }
}
