# For Loop
snippet for
alias for
    for (${2:i} = 0; $2 < ${1:count}; ${3:++}$2) {
        ${4:/* code */}
    }
# Custom For Loop
snippet forr
alias forr
    for (${1:i} = ${2:0}; ${3:$1 < 10}; ${4:++}$1) {
        ${5:/* code */}
    }
# printf
snippet fprintf
alias fpr
    fprintf(${1:stdout}, "${2:%s\n}", ${3:variables});
