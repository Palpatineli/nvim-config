# For Loop
snippet for
for (${2:i} = 0; $2 < ${1:count}; ${3:++}$2) {
	${4:/* code */}
}
endsnippet
# Custom For Loop
snippet forr
for (${1:i} = ${2:0}; ${3:$1 < 10}; ${4:++}$1) {
	${5:/* code */}
}
endsnippet
# printf
snippet fpr
fprintf(${1:stdout}, "${2:%s\n}", ${3:variables});
endsnippet
snippet ifndef
#ifndef ${1:name}_H
${2:content}
#define ${1}_H
#endif  // ${1}_H
endsnippet
